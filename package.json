    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.9.0",
    "xo": "*"
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
