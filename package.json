{
  "name": "deku-button",
  "version": "1.1.2",
  "description": "Button component for deku",
  "license": "MIT",
  "repository": "kevva/deku-button",
  "author": {
    "name": "Kevin MÃ¥rtensson",
    "email": "kevinmartensson@gmail.com",
    "url": "github.com/kevva"
  },
  "engines": {
    "node": ">=0.10.0"
